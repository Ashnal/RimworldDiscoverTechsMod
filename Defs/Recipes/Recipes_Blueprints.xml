<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
		<defName>NeolithicBlueprint</defName>
		<label>Create neolithic blueprint</label>
		<description>Allows you to create a blueprint with a technology from the neolithic era.</description>
		<jobString>Researching.</jobString>
		<workAmount>1500</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
    <workSkill>Intellectual</workSkill>
		<efficiencyStat>ButcheryFleshEfficiency</efficiencyStat>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <ingredients>
			<li>
				<filter>
					<categories>
						<li>Cloth</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
	<products>
		<NeolithicBlueprint>1</NeolithicBlueprint>
	</products>
	<recipeUsers>
		<li>SimpleResearchBench</li>
		<li>HiTechResearchBench</li>
	</recipeUsers>
	<researchPrerequisite>MicroelectronicsBasics</researchPrerequisite>
  </RecipeDef>

	
</Defs>