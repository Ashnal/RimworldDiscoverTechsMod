<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>TechnologyBlueprint</defName>
    <label>technology blueprint</label>
    <thingClass>ThingWithComps</thingClass>
    <description>blueprint prototype</description>
    <graphicData>
      <texPath>Things/Item/Blueprints/Blueprint_Old</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <itemGeneratorTags>
      <li>SpecialReward</li>
    </itemGeneratorTags>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>1</stackLimit>
    <tradeNeverStack>true</tradeNeverStack>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>1000</MarketValue>
      <MaxHitPoints>80</MaxHitPoints>
      <Mass>0.2</Mass>
      <Flammability>0.9</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <drawGUIOverlay>false</drawGUIOverlay>
    <tradeability>Sellable</tradeability>
    <techLevel>Neolithic</techLevel>
    <tradeTags>
      <li>Exotic</li>
    </tradeTags>
    <comps>
      <li Class="RimworldDiscoverTechs.CompProperties_BlueprintTech">
        <compClass>RimworldDiscoverTechs.CompBlueprintTech</compClass>
        <techLevels>
          <li>Neolithic</li>
          <li>Medieval</li>
          <li>Industrial</li>
          <li>Spacer</li>
        </techLevels>
      </li>
      <li Class="CompProperties_Usable">
        <compClass>RimworldDiscoverTechs.CompBlueprint</compClass>
        <useJob>UseTechnologyBlueprint</useJob>
        <useLabel>Use technology blueprint to discover a new technology</useLabel>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>RimworldDiscoverTechs.CompUseEffect_UnlockTechnology</compClass>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>CompUseEffect_DestroySelf</compClass>
      </li>
    </comps>
  </ThingDef>
  
