<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="ResourceBase">
    <defName>TechnologyBlueprintNeolithic</defName>
    <label>neolithic technology blueprint</label>
    <thingClass>ThingWithComps</thingClass>
    <description>A neolithic technology blueprint. By using it, you learn a research technology.</description>
    <graphicData>
      <texPath>Things/Item/Blueprints/Blueprint_Old</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	  <itemGeneratorTags>
      <li>SpecialReward</li>
    </itemGeneratorTags>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>5</stackLimit>
    <tradeNeverStack>true</tradeNeverStack>
	  <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>200</MarketValue>
      <MaxHitPoints>80</MaxHitPoints>
      <Mass>0.2</Mass>
	  <Flammability>0.9</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>TechnologyBlueprints</li>
    </thingCategories>
    <drawGUIOverlay>true</drawGUIOverlay>
    <tradeability>Stockable</tradeability>
    <techLevel>Neolithic</techLevel>
    <tradeTags>
      <li>Exotic</li>
    </tradeTags>
    <comps>
      <li Class="RimworldDiscoverTechs.CompProperties_BlueprintTech">
        <compClass>RimworldDiscoverTechs.CompBlueprintTech</compClass>
        <techLevels>
          <li>Neolithic</li>
        </techLevels>
      </li>
      <li Class="CompProperties_Usable">
        <compClass>RimworldDiscoverTechs.CompBlueprint</compClass>
        <useJob>UseTechnologyBlueprint</useJob>
        <useLabel>{0} technology blueprint</useLabel>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>RimworldDiscoverTechs.CompUseEffect_UnlockTechnology</compClass>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>CompUseEffect_DestroySelf</compClass>
      </li>
    </comps>
  </ThingDef>
  
